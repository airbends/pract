import java.io.*;
import java.util.*;

class Student
{
   int rollno;
   String name;
   static int count=0;

   Student()
   {
      rollno=0;
      name="";
      count++;
   }

   Student(int rollno,String name)
   {
      this.rollno = rollno;
      this.name = name;
      count++;
      System.out.println("Object Count = "+count);
   }

   public String toString()
   {
      return rollno+" "+name;
   }
}

class StudentMain
{
   public static void main(String args[])
   {
      int n = Integer.parseInt(args[0]);
      Student s[] = new Student[n];
      int k=1;

      for(int i=0;i<n;i++)
      {
         int r = Integer.parseInt(args[k]);
         String nm = args[k+1];
         s[i] = new Student(r,nm);
         System.out.println(s[i]);
         k+=2;
      }
   }
}

