import java.io.*;
import java.util.*;

class ColorList
{
  public static void main(String args[])
  {
    LinkedList<String> list=new LinkedList<String>();
    list.add("red");
    list.add("blue");
    list.add("yellow");
    list.add("orange");

    System.out.println("Forward:");
    Iterator it=list.iterator();
    while(it.hasNext()) System.out.println(it.next());

    System.out.println("Reverse:");
    ListIterator li=list.listIterator(list.size());
    while(li.hasPrevious()) System.out.println(li.previous());

    LinkedList<String> extra=new LinkedList<String>();
    extra.add("pink");
    extra.add("green");

    int pos=list.indexOf("yellow");
    list.addAll(pos,extra);

    System.out.println("After inserting new list:");
    for(String s:list) System.out.println(s);
  }
}

/*
cmd steps:
javac ColorList.java
java ColorList
*/
