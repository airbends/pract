import java.util.*;

class ListMain
{
  public static void main(String args[])
  {
    LinkedList<String> l=new LinkedList<String>();
    l.add("CPP"); l.add("Java"); l.add("Python"); l.add("PHP");
    System.out.println("Forward:");
    Iterator it=l.iterator(); while(it.hasNext()) System.out.println(it.next());
    System.out.println("Reverse:");
    ListIterator li=l.listIterator(l.size()); while(li.hasPrevious()) System.out.println(li.previous());
  }
}

/*
cmd:
javac ListMain.java
java ListMain
*/

