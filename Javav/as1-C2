import java.io.*;
import java.util.*;

class ReverseWords
{
    public static void main(String args[])
    {
        for(int i = 0; i < args.length; i++)
        {
            String word = args[i];
            String rev = "";

            for(int j = word.length() - 1; j >= 0; j--)
            {
                rev = rev + word.charAt(j);
            }

            System.out.println(rev);
        }
    }
}
